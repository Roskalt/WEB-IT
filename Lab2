#!/bin/bash

count=1

for param in "$*"
do
echo "\$* Parameter #$count = $param"
count=$(( $count + 1 ))
done

count=1

for param in "$@"
do
echo "\$@ Parameter #$count = $param"
count=$(( $count + 1 ))
done

user1@lab:~$ ./myscript 1 2 3 4 5
$* Parameter #1 = 1 2 3 4 5
$@ Parameter #1 = 1
